{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "0.1.0",
	"tasks": [
		{
			"taskName": "run build:w",
			"command": "npm",
			"isShellCommand": true,
			"showOutput": "always",
			"suppressTaskName": true,
			"args": ["run", "build:w"],
			"isBuildCommand": true,
			"problemMatcher": "$tsc"
		}/*,
		{
			"taskName": "tsc",
			"command": "tsc",
			"isShellCommand": true,
			"args": ["-w", "-p", ".", "--noEmit"],
			"showOutput": "silent",
			"isBackground": true,
			"problemMatcher": "$tsc-watch"
		}
		*/
	]
}
